algoritmo "QuanTinta"
var
   Lg, Al, Area, L: Real

inicio
      Escreva ("Qual a largura da parede? ")
      Leia (Lg)
      Escreva ("Qual a altura da parede? ")
      Leia (Al)
      Area <- Lg * Al
      Escreval ("A area total é de ", Area, "m2")
      L <- Area * 2
      Escreva ("A quantidade necessaria pra pintar ", area, " m2 são de ", L, " litros ")
      


fimalgoritmo